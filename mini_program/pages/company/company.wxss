@import "../../wxParse/wxParse.wxss";

.company-pic image {
    width: 100%;
    height: 360rpx;
}

.company-in {
    display: block;
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx;
    font-size: 28rpx;
    background-color: #fff;
    margin-top: -6rpx;
    text-align: justify;
    color:#777;
}

.company-range {
    width: 100%;
    font-size: 28rpx;
    color: #487fe4;
    box-sizing: border-box;
    padding: 16rpx;
    background-color: #fff;
    position: relative;
    margin-bottom:2rpx;
}
.company-range:before {
    content: "";
    display: block;
    width: 4rpx;
    height: 30rpx;
    background-color: #487fe4;
    position: absolute;
    top:22rpx;
    left: 10rpx;
}
.range-con {
    width: 100%;
    box-sizing: border-box;
    padding: 24rpx;
    background-color: #fff;
    font-size: 28rpx;
    text-align: justify;
    
}


.ewm-box {
    width: 100%;
    background-color: #fff;
    display: flex;
    justify-content: center;
    box-sizing: border-box;
    padding-bottom: 20rpx;
}

.ewm {
    text-align: center;
}

.call {
    width: 100%;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    background-color: #487fe4;
    color: #fff;
    font-size: 32rpx;
}


